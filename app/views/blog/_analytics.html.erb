<% if ENV["ANALYTICS_KEY_BLOG"].present? %>
<script
  async
  src="https://cdn.panelbear.com/analytics.js?site=<%= ENV["ANALYTICS_KEY_BLOG"] %>">
</script>
<script>
    window.panelbear = window.panelbear || function() {
      (window.panelbear.q = window.panelbear.q || []).push(arguments);
    };
    panelbear(
      'config',
      { site: '<%= ENV["ANALYTICS_KEY_BLOG"] %>', honorDNT: true }
    );
</script>
<% end %>
